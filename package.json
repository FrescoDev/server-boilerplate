    "bunyan": "^1.8.11",
    "bunyan-middleware": "^0.8.0",
    "cucumber": "3.2.1",
    "dyson": "0.10.0",
    "eslint": "2.11.1",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-plugin-import": "1.8.1",
    "expect": "1.20.2",
    "nock": "8.0.0",
    "nodemon": "1.11.0",
    "nsp": "2.6.1",
